			</dependency>
			<dependency>
				<groupId>${astrix.groupId}</groupId>
				<artifactId>astrix-service-registry-pu</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${astrix.groupId}</groupId>
				<artifactId>astrix-dashboard</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${astrix.groupId}</groupId>
				<artifactId>astrix-test-util</artifactId>
				<version>${project.version}</version>
			</dependency>
			
			<!-- GigaSpaces -->
			<dependency>
				<groupId>com.gigaspaces</groupId>
				<artifactId>gs-runtime</artifactId>
				<version>${gigaspaces.version}</version>
			</dependency>
			<dependency>
				<groupId>com.gigaspaces</groupId>
				<artifactId>gs-openspaces</artifactId>

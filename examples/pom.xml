<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.avanza.astrix</groupId>
		<artifactId>astrix-parent</artifactId>
		<version>0.14.8</version>
	</parent>
	<artifactId>astrix-examples-parent</artifactId>
	<packaging>pom</packaging>
	<modules>
		<module>lunch-parent</module>
		<module>lunch-grader-parent</module>
		<module>lunch-astrix-extensions</module>
		<module>example-application-runners</module>
		<module>lunch-web</module>
		<module>lunch-tests</module>
	</modules>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<java.version>1.7</java.version>
		<astrix.groupId>com.avanza.astrix</astrix.groupId>
		<log4j.version>1.2.16</log4j.version>
		<spring.version>3.2.4.RELEASE</spring.version>
		<spring.groupId>org.springframework</spring.groupId>
		<slf4j.version>1.6.4</slf4j.version>
		<gigaspaces.version>9.7.0-10560-RELEASE</gigaspaces.version>
		<rxjava.version>0.20.1</rxjava.version>
		<hystrix.version>1.3.17</hystrix.version>
		<jackson1.version>1.9.12</jackson1.version>
		<guava.version>14.0.1</guava.version>
		<junit.version>4.10</junit.version>
		<hamcrest.version>1.2.1</hamcrest.version>
		<lifecycle-mapping.version>1.0.0</lifecycle-mapping.version>
		<mockito.version>1.10.8</mockito.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>${astrix.groupId}</groupId>
				<artifactId>astrix-context</artifactId>
				<version>${project.version}</version>
